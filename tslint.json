{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-submodule-imports": [
      true,
      "antd"
    ],
    "jsx-no-multiline-js": [
      false
    ],
    "no-console": [
      false
    ],
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ],
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}